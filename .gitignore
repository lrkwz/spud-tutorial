/*.war

# plugin release files
/*.zip
/*.zip.sha1
/plugin.xml

# older plugin install locations
/plugins
/web-app/plugins

# "temporary" build files
/target

# other
.classpath

# Eclipse
*.pydevproject
.metadata
.gradle
bin/
target/
target-eclipse/
tmp/
*.tmp
*.bak
*.swp
*~.nib
local.properties
.settings/
.loadpath

# External tool builders
.externalToolBuilders/

